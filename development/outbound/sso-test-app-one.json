{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "sso-test-app-one",
  "description": "",
  "callbacks": [
    "https://herokuapp.com/saml/acs"
  ],
  "oidc_conformant": true,
  "is_first_party": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "logo_uri": "",
  "sso": false,
  "allowed_logout_urls": [],
  "allowed_clients": [],
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 3600,
    "secret_encoded": false
  },
  "client_aliases": [],
  "client_metadata": {
    "outbound_sso": "true",
    "partner_name": "Vinsol",
    "use_signed_cert": "true",
    "custom_certificate_name": "GR_SSO_ADMIN_OUTBOUND_ENCRYPTION_CERTIFICATE_TEST"
  },
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "regular_web",
  "grant_types": [
    "authorization_code",
    "implicit",
    "refresh_token",
    "client_credentials"
  ],
  "custom_login_page_on": false,
  "addons": {
    "samlp": {
      "createUpnClaim": false,
      "passthroughClaimsWithNoMapping": false,
      "mapIdentities": false,
      "signResponse": true,
      "dataMappings": [
        {
          "type": "static",
          "key": "partnerId",
          "value": "Vinsol"
        },
        {
          "type": "dynamic",
          "key": "userId",
          "value": "id"
        },
        {
          "type": "dynamic",
          "key": "firstName",
          "value": "first_name"
        },
        {
          "type": "dynamic",
          "key": "lastName",
          "value": "last_name"
        },
        {
          "type": "dynamic",
          "key": "dateOfBirth",
          "value": "dob",
          "format": "%Y%m%d"
        },
        {
          "type": "conditional",
          "key": "relationship",
          "value": "3",
          "conditions": [
            {
              "when": [
                {
                  "lhs": "relationship",
                  "opr": "eq",
                  "rhs": "Member"
                }
              ],
              "then": "1"
            },
            {
              "when": [
                {
                  "lhs": "relationship",
                  "opr": "eq",
                  "rhs": "Spouse"
                }
              ],
              "then": "2"
            }
          ]
        },
        {
          "type": "dynamic",
          "key": "relationshipRaw",
          "value": "relationship",
          "format": ""
        },
        {
          "type": "dynamic",
          "key": "customerName",
          "value": "customer_name",
          "format": ""
        }
      ]
    }
  }
}
